{
  "name": "Sinon",
  "version": "1.2.1",
  "description": "A video tool that's actually useful",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "win": "electron-packager . --app-version 1.2.1 --prune=true --platform win32 --icon ./imgs/sinon.ico --overwrite",
