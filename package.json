    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.11.0"
  },
  "dependencies": {
    "animejs": "^3.2.0",
    "electron-json-storage": "^4.1.8",
    "ffmpeg-static-electron": "^2.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "gifsicle": "^5.1.0",
    "got": "^11.5.1",
    "jquery": "^1.12.4"
  }
